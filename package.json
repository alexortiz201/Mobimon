{
  "name": "mobimon",
  "private": true,
  "version": "0.0.1",
  "description": "Mobimon",
  "main": "",
  "scripts": {
    "start": "node core/server/webpack-dev-server-init.js --env=dev",
    "start:dist": "node core/server/webpack-dev-server-init.js --env=dist",
    "watch": "watch 'clear && npm run -s test:core' ",
    "watch:cartridges": "watch 'clear && npm run -s test:cartridges' ",

    "test": "npm run test:core && npm run test:cartridges",
    "test:core": "babel-tape-runner \"core/**/**.test.js\" | tap-spec",
    "test:cartridges": "babel-tape-runner \"cartridges/**/**.test.js\" | tap-spec",

    "lint": "npm run lint:core && npm run lint:cartridges",
    "lint:core": "eslint ./core/public ./core/shared",
    "lint:cartridges": "eslint ./cartridges",

    "update": "updtr && depcheck --ignores='babelify' ",
    "clean": "rimraf dist/*",
    "copy": "mkdir -p ./dist && copyfiles -f ./core/public/index.html ./dist ",
    "build:dist": "npm run copy & NODE_ENV=production webpack --env=dist"
  },
  "repository": "https://github.com/alexortiz201/Mobimon",
  "keywords": [],
  "author": "Alex Ortiz",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-tape-runner": "^2.0.1",
    "cheerio": "^0.20.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "depcheck": "^0.6.4",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "updtr": "^0.2.1",
    "url-loader": "^0.5.7",
    "watch": "^0.19.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "history": "^3.0.0",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.2",
    "localforage-getitems": "^1.3.0",
    "lodash": "^4.15.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^4.2.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "tape": "^4.6.0"
  }
}
